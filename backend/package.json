{
  "name": "backend",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "api:login": "sh -c 'http -v --session=/tmp/better-auth-session POST http://localhost:3000/api/auth//sign-in/email email=\"$1\" password=\"$2\"' --",
    "api:register": "sh -c 'http POST http://localhost:3000/api/auth/sign-up/email name=\"$1\" email=\"$2\" password=\"$2\"' --",
    "api:me": "sh -c 'http -v --session=/tmp/better-auth-session http://localhost:3000/api/me'",
    "api:healthcheck": "sh -c 'http http://localhost:3000/api/healthcheck'",
    "api:logout": "sh -c 'http -v --session=/tmp/better-auth-session POST http://localhost:3000/api/auth/sign-out'"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.0",
    "@prisma/client": "^6.14.0",
    "better-auth": "^1.3.7",
    "hono": "^4.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@hono/eslint-config": "^2.0.3",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/node": "^20.11.17",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "prisma": "^6.14.0",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.40.0"
  }
}
